<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".usermanagment.privatedtetailssFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <EditText
            android:id="@+id/namebenefactor"
            android:layout_width="242dp"
            android:layout_height="63dp"
            android:layout_marginStart="84dp"
            android:ems="10"
            android:hint="name"
            android:inputType="textPersonName"
            app:layout_constraintStart_toStartOf="parent"
            tools:layout_editor_absoluteY="86dp" />

        <EditText
            android:id="@+id/phonebenefactor"
            android:layout_width="251dp"
            android:layout_height="59dp"
            android:layout_marginStart="80dp"
            android:layout_marginTop="248dp"
            android:ems="10"
            android:hint="phone number"
            android:inputType="phone"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/namebenefactor" />

        <Button
            android:id="@+id/savedatabtn"
            android:layout_width="193dp"
            android:layout_height="83dp"
            android:layout_marginStart="18dp"
            android:layout_marginBottom="56dp"
            android:text="Button"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="@+id/phonebenefactor" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>